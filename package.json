{
  "name": "back",
  "description": "Coloc project back-end part",
  "version": "0.0.1",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Tom Sampic",
    "email": "tom.sampic@insa-lyon.fr"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": ">=14.10.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "test": "yarn run lint && yarn run compile:full && yarn run jest",
    "test:coverage": "yarn run lint && yarn run compile:full && yarn run jest:coverage",
    "test:openHandles": "yarn run lint && yarn run compile:full && yarn run jest:openHandles",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
    "dev": "ts-node-dev --no-notify src/",
    "start": "yarn run compile:full && node lib/",
    "view:coverage": "serve coverage/lcov-report",
    "jest": "jest",
    "jest:coverage": "jest --coverage",
    "jest:openHandles": "jest --detectOpenHandles",
    "compile": "tsc",
    "compile:full": "shx rm -rf lib/ && tsc",
    "compile:watch": "onchange 'src/**/*.ts' -- yarn run compile"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/authentication": "^4.5.11",
    "@feathersjs/authentication-local": "^4.5.11",
    "@feathersjs/authentication-oauth": "^4.5.11",
    "@feathersjs/configuration": "^4.5.11",
    "@feathersjs/errors": "^4.5.11",
    "@feathersjs/express": "^4.5.11",
    "@feathersjs/feathers": "^4.5.11",
    "@feathersjs/socketio": "^4.5.11",
    "@feathersjs/transport-commons": "^4.5.11",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-sequelize": "^6.2.0",
    "helmet": "^4.3.1",
    "sequelize": "^6.3.5",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^5.0.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/rewire": "^2.5.28",
    "@types/serve-favicon": "^2.5.2",
    "@types/validator": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "axios": "^0.21.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-cucumber": "^3.0.1",
    "jest-extended": "^0.11.5",
    "jest-ts-auto-mock": "^2.0.0",
    "onchange": "^7.1.0",
    "prettier": "^2.2.1",
    "serve": "^11.3.2",
    "shx": "^0.3.3",
    "ts-auto-mock": "^3.1.2",
    "ts-jest": "^26.5.1",
    "ts-node-dev": "^1.1.1",
    "ttypescript": "^1.5.12",
    "typescript": "^4.1.3"
  }
}
